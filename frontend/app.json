{
  "expo": {
    "name": "PocketCloset",
    "slug": "pocketcloset",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pocketcloset",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pocketcloset.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "pocketcloset"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "name": "PocketCloset",
      "output": "static",
      "favicon": "./assets/images/favicon.ico",
      "bundler": "metro",
      "package": "com.pocketcloset.app"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "googleClientId": {
        "ios": "19430671882-0q85lonv98umce67r40911kp0dg83i44.apps.googleusercontent.com",
        "android": "19430671882-cktkuomgu4jdcsotnk3k5nik4662fivn.apps.googleusercontent.com",
        "web": "19430671882-bcl7etl8r900aa57ae5qskb9tti6l31q.apps.googleusercontent.com"
      },
      "router": {},
      "eas": {
        "projectId": "c85eaf7c-db82-498a-9bf5-05315a08f12a"
      }
    }
  }
}
